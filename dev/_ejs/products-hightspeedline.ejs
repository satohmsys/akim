<% 
	include( '_include/_function.ejs' );
	$active = 'products';
	$css = ['<link rel="stylesheet" href="/css/products.min.css" />'];
	$scripts = ['<script src="/js/slick/slick.min.js"></script>', '<script src="/js/fancybox/fancybox.min.js"></script>','<script src="/js/productdetail.js"></script>'];
	$src = '/img/products/';
%>
<!DOCTYPE html>
<html>
<head>
	<%- include( '_include/head.ejs' ); %>
</head>
<body class="page page--products:">
	<%- include( '_include/header.ejs' ); %>
	<!-- ***** mv -->
    <section class="subpageMv js-effect">
        <div class="wrap">
            <h1 class="subpageMv__copy">
                <span class="mask"><span class="mask__element">高速生産ライン</span></span>
            </h1>
        </div>
    </section>
    <!-- ///// -->
    <div class="breadcrumbSection">
        <div class="wrap">
            <ul class="breadcrumb">
                <li><a href="">TOP</a></li>
                <li><a href="">製品紹介</a></li>
                <li><a href="">小型水晶振動子高速生産ライン</a></li>
            </ul>
        </div>
    </div>
    <main class="mainContent">
        <!-- ***** 製品一覧 -->
        <section class="section section--lineup">
            <div class="wrap">
                <header class="section--lineup__index">
                    <h2 class="section--lineup__index__text">
                        小型水晶振動子<br class="-only-sp">高速生産ライン
                    </h2>
                    <p class="section--lineup__index__description">
                        タクト0.6secを達成したAKIMの高速生産ラインです。<br class="-no-sp">各機器をタップすると詳細をご覧いただけます。
                    </p>
                </header>
                <div class="lineupArea">
                    <div class="lineup">
                        <div class="lineup__item">
                            <a data-fancybox href="<%- $src %>line-slider-1.png" data-caption="ブランクマウンタ">
                                <figure class="lineup__item__img">
                                    <img src="<%- $src %>line-slider-1.png" alt="">
                                    <figcaption class="lineup__item__name">ブランクマウンタ</figcaption>
                                </figure>
                            </a>
                        </div>
                        <div class="lineup__item">
                            <a data-fancybox href="<%- $src %>line-slider-2.png" data-caption="クリーン硬化炉">
                                <figure class="lineup__item__img">
                                    <img src="<%- $src %>line-slider-2.png" alt="">
                                    <figcaption class="lineup__item__name">クリーン硬化炉</figcaption>
                                </figure>
                            </a>
                        </div>
                        <div class="lineup__item">
                            <a data-fancybox href="<%- $src %>line-slider-3.png" data-caption="F調ローダ/アンローダ">
                                <figure class="lineup__item__img">
                                    <img src="<%- $src %>line-slider-3.png" alt="">
                                    <figcaption class="lineup__item__name">F調ローダ/アンローダ</figcaption>
                                </figure>
                            </a>
                        </div>
                        <div class="lineup__item">
                            <a data-fancybox href="<%- $src %>line-slider-4.png" data-caption="リッド仮付け装置">
                                <figure class="lineup__item__img">
                                    <img src="<%- $src %>line-slider-4.png" alt="">
                                    <figcaption class="lineup__item__name">リッド仮付け装置</figcaption>
                                </figure>
                            </a>
                        </div>
                        <div class="lineup__item">
                            <a data-fancybox href="<%- $src %>line-slider-5.png" data-caption="真空アニール装置">
                                <figure class="lineup__item__img">
                                    <img src="<%- $src %>line-slider-5.png" alt="">
                                    <figcaption class="lineup__item__name">真空アニール装置</figcaption>
                                </figure>
                            </a>
                        </div>
                        <div class="lineup__item">
                            <a data-fancybox href="<%- $src %>line-slider-6.png" data-caption="直交真空シーム溶接装置">
                                <figure class="lineup__item__img">
                                    <img src="<%- $src %>line-slider-6.png" alt="">
                                    <figcaption class="lineup__item__name">直交真空シーム溶接装置</figcaption>
                                </figure>
                            </a>
                        </div>
                        <div class="lineup__item">
                            <a data-fancybox href="<%- $src %>line-slider-7.png" data-caption="抜き取り装置">
                                <figure class="lineup__item__img">
                                    <img src="<%- $src %>line-slider-7.png" alt="">
                                    <figcaption class="lineup__item__name">抜き取り装置</figcaption>
                                </figure>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- /////  -->
    </main>
	<%- include( '_include/footer.ejs' ); %>
</body>
</html>